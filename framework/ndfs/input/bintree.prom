/* A binary tree of depth max, with accepting states at depth accept */

int accept = 13; /* smaller or equal to max */
int max = 19; /* smaller or equal to 30 */

init {
	int d = 0; /* depth */
	int p = 0; /* position */

	do
	:: d < accept ->
		if
		:: p = p + (1 << d); d++
		:: d++
		fi
	:: else -> break
	od

	accept: skip;

	do
	:: d < max ->
		if
		:: p = p + (1 << d); d++
		:: d++
		fi
	:: else -> break
	od

	if
	:: false
	fi
}
